<UserControl x:Class="buildlite.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="480" d:DesignWidth="640" HorizontalAlignment="Center" VerticalAlignment="Center">

    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel Name="gameviewpanel" Loaded="Page_Loaded" Visibility="Visible" Background="#FF0A0A0A">
            <Image Height="480"  Visibility="Visible" Name="viewportimg" Stretch="Fill" Width="641" HorizontalAlignment="Right" />

        </StackPanel>
    </Grid>
</UserControl>
